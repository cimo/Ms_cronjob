{
    "name": "ms_cronjob",
    "version": "1.0.0",
    "description": "Microservice cronjob.",
    "author": "cimo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cimo/Ms_cronjob.git"
    },
    "type": "module",
    "scripts": {
        "preinstall": "npx -y npm-force-resolutions",
        "remove_dist": "node -e \"const fs = require('fs'); try{ fs.rmdirSync('./dist/', { recursive: true }) } catch{}; process.exit(0);\"",
        "check": "eslint --no-cache --config eslint.config.js",
        "build_tsc": "npm run remove_dist && tsc --build tsconfig.tsc.json",
        "build": "npm run build_tsc && npm run check",
        "execute": "nodemon --config nodemon.json"
    },
    "dependencies": {
        "@cimo/environment": "1.0.23",
        "@cimo/cronjob": "1.0.10"
    },
    "devDependencies": {
        "@types/node": "22.10.2",
        "@typescript-eslint/parser": "8.24.1",
        "@typescript-eslint/eslint-plugin": "8.24.1",
        "typescript": "5.7.2",
        "nodemon": "3.1.9",
        "prettier": "3.4.2",
        "eslint": "9.31.0",
        "eslint-plugin-prettier": "5.2.1"
    },
    "resolutions": {}
}
